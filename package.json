    "@smartcar/auth": "^2.9.0",
    "expo": "~46.0.9",
    "expo-status-bar": "~1.4.0",
    "formik": "^2.2.9",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "react-native-paper": "^4.12.5",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-vector-icons": "^9.2.0",
    "react-navigation-tabs": "^2.11.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "concurrently": "^7.4.0",
    "localtunnel": "^2.0.2"
