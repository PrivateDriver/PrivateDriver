{
  "name": "privatedriver",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "concurrently --kill-others --raw \"npm:expose-tunnel\" \"expo start\"",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "expose-tunnel": "lt --port 3000 --subdomain limo-app-server"
  },
  "dependencies": {
    "@smartcar/auth": "^2.9.0",
    "expo": "~46.0.9",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "concurrently": "^7.4.0",
    "localtunnel": "^2.0.2"
  },
  "private": true
}
